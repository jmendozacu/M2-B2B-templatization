<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
  <head>
     <script src="Dckap_TaxExemptionCertificate::js/tax_cert.js"/>
  </head>
    <update handle="customer_account"/>   
   <body>
        <referenceBlock name="head.components">
            <block class="Magento\Framework\View\Element\Js\Components" name="customer_account_address_edit_head_components" template="Magento_Customer::js/components.phtml"/>
        </referenceBlock>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Upload Tax Exemption Certificate</argument>
            </action>
        </referenceBlock>
       <referenceContainer name="content">
            <block class="Dckap\TaxExemptionCertificate\Block\Tax" name="customer_tax" template="tax/taxform.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>
