<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
   <head>
     <script src="Dckap_PurchaseReport::js/jspdf/jspdf.js"/>
     <script src="Dckap_PurchaseReport::js/jspdf.plugin.autotable.src.js"/>
  </head>
    <update handle="customer_account"/>   
   <body>
        <referenceBlock name="head.components">
            <block class="Magento\Framework\View\Element\Js\Components" name="customer_account_address_edit_head_components" template="Magento_Customer::js/components.phtml"/>
        </referenceBlock>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Purchase Reports</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Dckap\PurchaseReport\Block\Purchasereport" name="purchase_report" template="purchase_report.phtml" cacheable="false" />
        </referenceContainer>
    </body>
</page>
